{
  "name": "@canva-public/rocketbot",
  "version": "1.0.0",
  "description": "A lambda function to enable comment-driven actions via github",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write .",
    "pretest": "prettier --check . && yarn eslint",
    "test": "jest --ci --coverage",
    "test:watch": "jest --watch --notify",
    "prepack": "tsc --declarationDir dist --declaration",
    "prepare": "rm -rf ./dist && husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/canva-public/rocketbot.git"
  },
  "keywords": [
    "lambda",
    "github",
    "comments",
    "buildkite"
  ],
  "author": "Canva Pty Ltd.",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@octokit/types": "^6.14.2",
    "@octokit/webhooks-schemas": "^3.71.2",
    "@octokit/webhooks-types": "^3.71.2",
    "@types/aws-lambda": "^8.10.76",
    "@types/jest": "^26.0.22",
    "@types/node": "12.x",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "husky": ">=6",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "nock": "^13.0.11",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": "12.x || 14.x"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,css,md,ts}": "prettier --write"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6"
  }
}
