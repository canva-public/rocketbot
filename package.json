{
  "name": "@canva/rocketbot",
  "version": "0.0.0",
  "description": "A lambda function to enable comment-driven actions via github",
  "scripts": {
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write .",
    "pretest": "prettier --check . && yarn eslint",
    "test": "jest --ci --coverage",
    "test:watch": "jest --watch --notify",
    "build": "rimraf ./dist && rollup -c",
    "prepare": "husky install",
    "test-dist": "sam local invoke 'RocketBot' -e event.json --env-vars env.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/canva-public/rocketbot.git"
  },
  "keywords": [
    "lambda",
    "github",
    "comments",
    "buildkite"
  ],
  "author": "Canva Pty Ltd.",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@octokit/types": "^6.14.2",
    "@octokit/webhooks-schemas": "^3.72.0",
    "@octokit/webhooks-types": "^3.72.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/aws-lambda": "^8.10.76",
    "@types/jest": "^26.0.23",
    "@types/node": "10.x",
    "@types/pino": "^6.3.8",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "husky": ">=6",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "nock": "^13.0.11",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.47.0",
    "ts-jest": "^26.5.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "yaml-cfn": "^0.3.0"
  },
  "engines": {
    "node": "10.x || 12.x || 14.x"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,css,md,ts}": "prettier --write"
  },
  "dependencies": {
    "@octokit/auth-app": "^3.4.0",
    "@octokit/rest": "^18.5.3",
    "aws-lambda": "^1.0.6",
    "pino": "^6.11.3",
    "pino-lambda": "^2.0.0",
    "pino-pretty": "^4.7.1"
  }
}
