{
  "name": "@canva/rocketbot",
  "version": "0.0.0",
  "description": "A lambda function to enable comment-driven actions via github",
  "scripts": {
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write .",
    "pretest": "prettier --check . && yarn eslint",
    "test": "jest --ci --coverage",
    "test:watch": "jest --watch --notify",
    "build": "rimraf ./dist && rollup -c",
    "prepare": "husky install",
    "pretest-dist": "yarn build",
    "test-dist": "sam local invoke 'RocketBot' -e event.json --env-vars env.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/canva-public/rocketbot.git"
  },
  "keywords": [
    "lambda",
    "github",
    "comments",
    "buildkite"
  ],
  "author": "Canva Pty Ltd.",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@octokit/graphql-schema": "^10.39.1",
    "@octokit/types": "^6.14.2",
    "@octokit/webhooks-schemas": "^3.75.0",
    "@octokit/webhooks-types": "^3.75.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/aws-lambda": "^8.10.76",
    "@types/git-url-parse": "^9.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash.sortby": "^4.7.6",
    "@types/node": "10.x",
    "@types/pino": "^6.3.8",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jest": "^24.3.6",
    "husky": ">=6",
    "jest": "^26.6.3",
    "lint-staged": ">=11",
    "nock": "^13.0.11",
    "prettier": "2.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.48.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "yaml-cfn": "^0.3.0"
  },
  "engines": {
    "node": "10.x || 12.x || 14.x"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.{js,css,md,ts}": "prettier --write"
  },
  "dependencies": {
    "@octokit/auth-app": "^3.4.0",
    "@octokit/graphql": "^4.6.1",
    "@octokit/rest": "^18.5.3",
    "@octokit/webhooks-methods": "^1.0.0",
    "aws-lambda": "^1.0.6",
    "git-url-parse": "^11.4.4",
    "got": "^11.8.2",
    "lodash.sortby": "^4.7.0",
    "memoize-one": "^5.2.1",
    "pino": "^6.11.3",
    "pino-lambda": "^2.0.0",
    "pino-pretty": "^4.8.0",
    "zod": "^3.0.0-beta.1"
  }
}
